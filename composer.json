{
    "name": "hskrasek/arazzo-parser",
    "description": "This is my package arazzo-parser",
    "keywords": [
        "HSkrasek",
        "arazzo-parser"
    ],
    "homepage": "https://github.com/hskrasek/arazzo-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Hunter Skrasek",
            "email": "hunterskrasek@me.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "justinrainbow/json-schema": "^6.2"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "HSkrasek\\Arazzo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HSkrasek\\Arazzo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
